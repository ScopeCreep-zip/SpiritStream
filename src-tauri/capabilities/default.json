{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "SpiritStream default capabilities",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "fs:allow-app-read",
    "fs:allow-app-write",
    "fs:allow-read",
    "shell:default",
    "shell:allow-spawn",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
