{
  "name": "@scopecreep-zip/spiritstream",
  "version": "0.1.0",
  "description": "SpiritStream - Multi-Destination Streaming Application",
  "private": false,
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "generate:platforms": "tsx scripts/generate-platforms.ts",
    "predev": "npm run generate:platforms",
    "prebuild": "npm run generate:platforms",
    "tauri": "tauri",
    "dev": "tauri dev",
    "build": "tauri build",
    "build:debug": "tauri build --debug",
    "backend:dev": "cargo run --manifest-path src-tauri/Cargo.toml --bin server",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "typecheck": "tsc -p tsconfig.frontend.json --noEmit",
    "check": "cargo check --manifest-path src-tauri/Cargo.toml",
    "lint": "eslint src-frontend --ext .ts,.tsx",
    "lint:fix": "eslint src-frontend --ext .ts,.tsx --fix",
    "format": "prettier --write \"src-frontend/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src-frontend/**/*.{ts,tsx,css,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScopeCreep-zip/SpiritStream.git"
  },
  "keywords": [
    "streaming",
    "rtmp",
    "ffmpeg",
    "tauri",
    "multi-stream",
    "broadcast",
    "obs-alternative",
    "desktop-app"
  ],
  "author": "SpiritStream Contributors",
  "license": "ISC",
  "files": [
    "dist",
    "src-frontend",
    "src-tauri/src",
    "src-tauri/Cargo.toml",
    "src-tauri/tauri.conf.json",
    "src-tauri/icons",
    "src-tauri/build.rs",
    "config",
    "vite.config.ts",
    "tsconfig.frontend.json",
    "tsconfig.node.json",
    "index.html",
    "README.md",
    "LICENSE"
  ],
  "bugs": {
    "url": "https://github.com/ScopeCreep-zip/SpiritStream/issues"
  },
  "homepage": "https://github.com/ScopeCreep-zip/SpiritStream#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^22.13.9",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^17.0.0",
    "prettier": "^3.4.0",
    "puppeteer": "^24.34.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "clsx": "^2.1.1",
    "i18next": "^25.7.3",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.1",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  }
}
